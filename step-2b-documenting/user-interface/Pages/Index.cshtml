@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<div class="table100 ver1 m-b-110">
    <div class="table100-head">
        <table>
            <thead>
                <tr class="row100 head">
                    <th class="cell100 column1">Provider</th>
                    <th class="cell100 column2">Type</th>
                    <th class="cell100 column3">Max. Sum Insurable</th>
                    <th class="cell100 column4">Monthly Premium</th>
                    <th class="cell100 column5">GST %</th>
                    <th class="cell100 column6">Amount payable each month</th>
                </tr>
            </thead>
        </table>
    </div>
    <div class="table100-body js-pscroll ps ps--active-y">
        <table>
            <tbody>
                @foreach (var item in @Model.Products)
                {
                <tr class="row100 body">
                    <td class="cell100 column1">@item.provider</td>
                    <td class="cell100 column2">@item.type</td>
                    <td class="cell100 column3">@item.maxSumInsurable</td>
                    <td class="cell100 column4">@item.monthlyPremium</td>
                    <td class="cell100 column5">@item.gst</td>
                    <td class="cell100 column6">@item.total</td>
                </tr>
                }
            </tbody>
        </table>
        <div class="ps__rail-x" style="left: 0px; bottom: 0px;"><div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__rail-y" style="top: 0px; height: 585px; right: 5px;"><div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 293px;"></div></div>
    </div>
</div>
